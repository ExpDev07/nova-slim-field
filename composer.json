{
    "name": "expdev07/nova-slim-field",
    "description": "Laravel Nova field for cropping images using the \"Slim Image Cropper\" (https://pqina.nl/slim/). This is an alternative to Doka (now Pintura) for those who want a less expensive cropping experience.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "image",
        "cropping",
        "slim",
        "admin",
        "expdev07"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "ExpDev07\\NovaSlimField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "ExpDev07\\NovaSlimField\\SlimFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
